<h1>Nueva Provincia</h1>

  <%= form_for :provincium, :url => provincia_path do |f| -%>
	
	  
	<fieldset>
		<legend><h2>Formulario</h2></legend>
		<p>
			<%= label_tag 'Nombre' %><br />
		    <%= f.text_field :Nombre , :autocomplete => :off%>
		</p>
    </fieldset>
  
  <p><%= submit_tag 'Crear Provincia' %></p>
  <% end %>

<%= link_to 'Listado de Provincias', provincia_path %>
